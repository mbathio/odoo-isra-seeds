<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Séquences pour les lots de semences par niveau -->
        <record id="seq_seed_lot_go" model="ir.sequence">
            <field name="name">Lot Semences GO</field>
            <field name="code">seed.lot.go</field>
            <field name="prefix">SL-GO-%(year)s-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
