<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Variétés de semences -->
        <record id="variety_sahel108" model="seed.variety">
            <field name="code">sahel108</field>
            <field name="name">Sahel 108</field>
            <field name="crop_type">rice</field>
            <field name="description">Variété de cycle court (100-110 jours) adaptée aux zones irriguées du Nord</field>
            <field name="maturity_days">105</field>
            <field name="yield_potential">9.5</field>
            <field name="resistances">Blast, Virus de la panachure jaune</field>
            <field name="origin">AfricaRice</field>
            <field name="release_year">1994</field>
        </record>

        <record id="variety_sahel202" model="seed.variety">
            <field name="code">sahel202</field>
            <field name="name">Sahel 202</field>
            <field name="crop_type">rice</field>
            <field name="description">Variété améliorée à haut rendement, bien adaptée aux conditions sahéliennes</field>
            <field name="maturity_days">125</field>
            <field name="yield_potential">10.0</field>
            <field name="resistances">Blast, Pyriculariose</field>
            <field name="origin">ISRA/AfricaRice</field>
            <field name="release_year">2007</field>
        </record>

        <record id="variety_zm309" model="seed.variety">
            <field name="code">zm309</field>
            <field name="name">ZM309</field>
            <field name="crop_type">maize</field>
            <field name="description">Variété de maïs tolérante à la sécheresse, adaptée aux zones semi-arides</field>
            <field name="maturity_days">95</field>
            <field name="yield_potential">7.2</field>
            <field name="resistances">Streak, Rouille</field>
            <field name="origin">IITA</field>
            <field name="release_year">2012</field>
        </record>

        <record id="variety_7333" model="seed.variety">
            <field name="code">73-33</field>
            <field name="name">73-33</field>
            <field name="crop_type">peanut</field>
            <field name="description">Variété d'arachide traditionnelle du Sénégal, bien adaptée aux zones sahéliennes</field>
            <field name="maturity_days">90</field>
            <field name="yield_potential">3.5</field>
            <field name="resistances"></field>
            <field name="origin">ISRA</field>
            <field name="release_year">1973</field>
        </record>

        <!-- Multiplicateurs -->
        <record id="multiplier_ibrahima" model="res.partner">
            <field name="name">Ibrahima Ba</field>
            <field name="is_multiplier">True</field>
            <field name="street">Dagana, Saint-Louis</field>
            <field name="phone">77 123 45 67</field>
            <field name="email">ibrahima@example.com</field>
            <field name="years_experience">8</field>
            <field name="certification_level">expert</field>
            <field name="multiplier_status">active</field>
            <field name="supplier_rank">1</field>
        </record>

        <record id="multiplier_aminata" model="res.partner">
            <field name="name">Aminata Diallo</field>
            <field name="is_multiplier">True</field>
            <field name="street">Podor, Saint-Louis</field>
            <field name="phone">77 234 56 78</field>
            <field name="email">aminata@example.com</field>
            <field name="years_experience">5</field>
            <field name="certification_level">intermediate</field>
            <field name="multiplier_status">active</field>
            <field name="supplier_rank">1</field>
        </record>

        <!-- Parcelles -->
        <record id="parcel_dagana01" model="agricultural.parcel">
            <field name="name">Parcelle Dagana 01</field>
            <field name="multiplier_id" ref="multiplier_ibrahima"/>
            <field name="area">5.2</field>
            <field name="latitude">16.5182</field>
            <field name="longitude">-15.5046</field>
            <field name="soil_type">clay_loamy</field>
            <field name="irrigation_system">drip</field>
            <field name="address">Zone agricole de Dagana, Saint-Louis</field>
            <field name="status">available</field>
        </record>

        <record id="parcel_podor02" model="agricultural.parcel">
            <field name="name">Parcelle Podor 02</field>
            <field name="multiplier_id" ref="multiplier_aminata"/>
            <field name="area">3.8</field>
            <field name="latitude">16.6518</field>
            <field name="longitude">-14.9592</field>
            <field name="soil_type">loamy</field>
            <field name="irrigation_system">sprinkler</field>
            <field name="address">Zone agricole de Podor, Saint-Louis</field>
            <field name="status">available</field>
        </record>

